{
    "loss": {
        "otype": "RelativeL2Luminance"
    },
    "optimizer": {
        "otype": "ExponentialDecay",
        "decay_start": 4000,
        "decay_interval": 4000,
        "decay_base": 0.33,
        "nested": {
            "otype": "Adam",
            "learning_rate": 1e-2,
            "beta1": 0.9,
            "beta2": 0.99,
            "epsilon": 1e-15,
            "l2_reg": 1e-6
        }
    },
    "encoding": {
        "otype": "Composite",
        "nested": [
            {
                "n_dims_to_encode": 3,
                "otype": "HashGrid",
                "per_level_scale": 2.0,
                "log2_hashmap_size": 15,
                "base_resolution": 16,
                "n_levels": 16
            }
        ]
    },
    "network": {
        "otype": "FullyFusedMLP",
        "activation": "ReLU",
        "output_activation": "None",
        "n_neurons": 64,
        "n_hidden_layers": 2
    }
}